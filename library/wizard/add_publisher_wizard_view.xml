<?xml version="1.0"?>
<odoo>
    <data>

        <record id="publisher_wizard_view_form" model="ir.ui.view">
            <field name="name">publisher_wizard_view_form</field>
            <field name="model">add.publisher.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group class="oe_title">
<!--                        <field name="book_ids" invisible="1"/>-->
                        <field name="publisher_id"/>

                    </group>
                    <footer>
                        <button string="Add Publisher" name="action_add_publisher" type="object"
                                class="oe_highlight" data-hotkey="q"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel" data-hotkey="z"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>